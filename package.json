{
  "name": "slj-center-erp",
  "private": true,
  "version": "1.0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:clear": "vite --force",
    "dev:bun": "bun --bun vite",
    "build": "tsc -b && vite build",
    "build:clean": "rimraf dist && tsc -b && vite build",
    "build:bun": "bun run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "docker:build": "docker build -t slj-frontend .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t slj-frontend:dev .",
    "docker:run": "docker run -p 8080:80 slj-frontend",
    "docker:run:dev": "docker run -p 8080:80 slj-frontend:dev",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:dev:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:dev:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:dev:restart": "docker-compose -f docker-compose.dev.yml restart",
    "docker:dev:clean": "docker-compose -f docker-compose.dev.yml down --volumes --remove-orphans",
    "docker:preview": "docker-compose -f docker-compose.dev.yml --profile preview up --build frontend-preview",
    "security:headers": "echo 'Test security headers at: https://securityheaders.com/ or https://observatory.mozilla.org/'",
    "clean": "rimraf dist node_modules/.vite",
    "clean:docker": "docker system prune -f && docker volume prune -f",
    "setup:dev": "bun install && bun run docker:dev",
    "health:dev": "curl -f http://localhost:5173 || echo 'Dev server not running'"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.14",
    "@types/node": "^24.0.3",
    "@types/react-helmet": "^6.1.11",
    "@uidotdev/usehooks": "^2.4.1",
    "@vitejs/plugin-react": "^4.6.0",
    "add": "^2.0.6",
    "antd": "^5.26.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "^19.1.0",
    "react-cookie": "^8.0.1",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^7.1.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^16.3.0",
    "sass-embedded": "^1.83.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.18.2",
    "vite": "^7.0.0"
  },
  "trustedDependencies": [
    "@swc/core"
  ]
}
